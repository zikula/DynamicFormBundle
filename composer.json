{
    "name": "zikula/dynamic-form-property-bundle",
    "type": "symfony-bundle",
    "description": "A Symfony bundle providing dynamic properties to Symfony forms.",
    "homepage": "https://ziku.la",
    "license": "MIT",
    "authors": [
        {
            "name": "Craig Heydenburg",
            "email": "craigh@mac.com"
        },
        {
            "name": "Axel Guckelsberger",
            "email": "info@guite.de"
        }
    ],
    "require": {
        "php": "^8.0",
        "doctrine/orm": "^2.11",
        "symfony/asset": "^5.4",
        "symfony/contracts": "^2",
        "symfony/form": "^5.4",
        "symfony/framework-bundle": "^5.4",
        "symfony/intl": "^5.4",
        "symfony/string": "^5.4",
        "symfony/validator": "^5.4"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^3.8",
        "phpunit/phpunit": "^9.5",
        "roave/security-advisories": "dev-latest"
    },
    "config": {
        "sort-packages": true
    },
    "autoload": {
        "psr-4": { "Zikula\\Bundle\\DynamicFormPropertyBundle\\": "src/" }
    },
    "autoload-dev": {
        "psr-4": { "Zikula\\Bundle\\DynamicFormPropertyBundle\\Tests\\": "tests/" }
    }
}
